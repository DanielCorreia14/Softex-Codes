
namespace x
{
    class FrangoAssado implements Sanduiche
    {
        double _preco = 0.99;
        string _descricao = "adicional de pepperoni";
       
        public addpeperoni(Sanduiche sandu)
        {
           _sandu = sandu;
        }
       
        public override double Preco
        {
            get
            {
                return sandu.preco + _preco;
            }
        }
        public override string Descricao
        {
            get
            {
                return _sandu.Descricao. + _descricao;
            }
        }
    }
}
