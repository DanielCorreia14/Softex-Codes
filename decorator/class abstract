
namespace x
{
    public abstract class Sanduiche
    {
        private double _preco = 0.00;
        private string _descricao = "Class abstract"

        public virtual double Preco
        {
            get { return _preco; }
        }
        public virtual string Descricao
        {
            get { return _descricao; }
        }


    }

    
}
